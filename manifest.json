{
  "manifest_version": 3,
  "name": "Reminder",
  "version": "1.0",
  "description": "Shows 'I love you' when you open the first tab.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}